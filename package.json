{
  "dependencies": {
    "axios": "^0.19.0",
    "mockjs": "^1.1.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^13.7.0",
    "@types/warning": "^3.0.0",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src/*.ts --out-dir lib --extensions .ts",
    "clean": "rimraf lib",
    "format": "prettier --write \"**/*{.js,.ts}\"",
    "lint": "npm run format && eslint --fix --ext \".js,.ts\" .",
    "test": "jest"
  },
  "name": "nuomi-request",
  "description": "axios+jsonp+mock封装的请求库 # 安装 ``` yarn add nuomi-request ```",
  "version": "1.0.0",
  "main": "lib/index.js",
  "typings": "global.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuomijs/nuomi-request.git"
  },
  "keywords": [
    "nuomi",
    "nuomi-request",
    "request",
    "axios",
    "jsonp"
  ],
  "author": "yinjiazeng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuomijs/nuomi-request/issues"
  },
  "homepage": "https://github.com/nuomijs/nuomi-request#readme"
}
